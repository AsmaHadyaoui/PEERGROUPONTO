@prefix : <http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#> .

<http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25> rdf:type owl:Ontology ;
                                                                           owl:imports <http://xmlns.com/foaf/0.1/> .

#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#achievesLearningOutcome
:achievesLearningOutcome rdf:type owl:ObjectProperty ,
                                  owl:FunctionalProperty ,
                                  owl:AsymmetricProperty ,
                                  owl:IrreflexiveProperty ;
                         rdfs:domain :Learner ;
                         rdfs:range :LearningOutcomes ;
                         <http://purl.org/dc/elements/1.1/description> "This property indicates that a learner has met specific learning objectives, demonstrating mastery of the required skills or knowledge" ;
                         rdfs:comment "Denotes the relationship between a learner and a learning outcome that they have successfully achieved" ;
                         owl:versionInfo "Version 1.2 - Enhanced to reflect its use in learner achievement tracking" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#alignsWithOutcome
:alignsWithOutcome rdf:type owl:ObjectProperty ,
                            owl:AsymmetricProperty ,
                            owl:IrreflexiveProperty ;
                   rdfs:domain :LearningResources ;
                   rdfs:range :LearningOutcomes ;
                   <http://purl.org/dc/elements/1.1/description> "Captures the relationship between learning resources and their associated outcomes, ensuring that all resources are effectively contributing to the predefined educational objectives" ;
                   rdfs:comment "Indicates the alignment of specific learning resources with intended learning outcomes. This property is used to establish connections between materials and the educational goals they are designed to achieve" ;
                   owl:versionInfo "Version 1.0 - Initial alignment between resources and outcomes for ontology validation" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#appliesToProject
:appliesToProject rdf:type owl:ObjectProperty ,
                           owl:AsymmetricProperty ,
                           owl:IrreflexiveProperty ;
                  rdfs:domain :AssessmentCriteria ;
                  rdfs:range :ProjectBasedLearning ;
                  <http://purl.org/dc/elements/1.1/description> "Describes the applicability of defined assessment criteria to a particular project, ensuring alignment with learning objectives in a project-based learning framework" ;
                  rdfs:comment "Indicates the association of specific assessment criteria with a project in the context of project-based learning" ;
                  owl:versionInfo "Version 1.0 - Initial definition: Establishes the connection between assessment criteria and project activities in collaborative learning environments" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#collaboratesOnProject
:collaboratesOnProject rdf:type owl:ObjectProperty ,
                                owl:SymmetricProperty ,
                                owl:IrreflexiveProperty ;
                       rdfs:domain :Group ;
                       rdfs:range :ProjectBasedLearning ;
                       <http://purl.org/dc/elements/1.1/description> "Indicates group involvement in a project, emphasizing teamwork and mutual contributions" ;
                       rdfs:comment "Describes the collaborative engagement of a group in a specific project aimed at achieving shared learning outcomes" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#evaluatesCriteria
:evaluatesCriteria rdf:type owl:ObjectProperty ,
                            owl:AsymmetricProperty ,
                            owl:IrreflexiveProperty ;
                   rdfs:domain :PeerFeedback ;
                   rdfs:range :AssessmentCriteria ;
                   <http://purl.org/dc/elements/1.1/description> "This property represents the connection where peer feedback is aligned with established assessment criteria, ensuring that the evaluations are consistent and comprehensive across different projects and learning outcomes" ;
                   rdfs:comment "Indicates the relationship between peer feedback and the specific assessment criteria used for evaluation. This property links the feedback process with predefined benchmarks to ensure objective and structured assessments" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#formsGroup
:formsGroup rdf:type owl:ObjectProperty ,
                     owl:AsymmetricProperty ,
                     owl:IrreflexiveProperty ;
            rdfs:domain :IntelligentGroupPairing ;
            rdfs:range :Group ;
            <http://purl.org/dc/elements/1.1/description> "Indicates the association between a grouping mechanism or algorithm and the resulting group" ;
            rdfs:comment "Represents the process by which a group is created or assembled within the intelligent grouping framework" ;
            owl:versionInfo "Version 1.0 - Initial definition for intelligent group formation" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#hasGroup
:hasGroup rdf:type owl:ObjectProperty ;
          owl:inverseOf :isMemberOf ;
          rdf:type owl:FunctionalProperty ,
                   owl:AsymmetricProperty ,
                   owl:IrreflexiveProperty ;
          rdfs:domain :Learner ;
          rdfs:range :Group ;
          <http://purl.org/dc/elements/1.1/description> "This property links a learner to a group they belong to, used to track group memberships in collaborative activitie" ;
          rdfs:comment "Represents the relationship between a learner and their associated group in collaborative learning" ;
          owl:versionInfo "Version 1.0 - Initial release: Captures group assignment relationships for learners in collaborative contexts" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#hasLearner
:hasLearner rdf:type owl:ObjectProperty ,
                     owl:AsymmetricProperty ,
                     owl:IrreflexiveProperty ;
            rdfs:domain :Group ;
            rdfs:range :Learner ;
            <http://purl.org/dc/elements/1.1/description> "Specifies the learners associated with a particular group for collaborative learning or assessment purposes" ;
            rdfs:comment "Defines the relationship between a group and its individual members categorized as learners" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#hasPerformanceMetric
:hasPerformanceMetric rdf:type owl:ObjectProperty ,
                               owl:AsymmetricProperty ,
                               owl:IrreflexiveProperty ;
                      rdfs:domain :Group ;
                      rdfs:range :GroupPerformance ;
                      <http://purl.org/dc/elements/1.1/description> "Defines the measurable criteria or metrics associated with a group's performance evaluation" ;
                      rdfs:comment "Represents the association of a group with specific performance metrics used to evaluate their effectiveness and outcomes" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#involvesGroup
:involvesGroup rdf:type owl:ObjectProperty ,
                        owl:AsymmetricProperty ,
                        owl:IrreflexiveProperty ;
               rdfs:domain :CollaborativeLearning ;
               rdfs:range :Group ;
               <http://purl.org/dc/elements/1.1/description> "Defines the association between collaborative learning activities and the groups of learners engaged in these activities" ;
               rdfs:comment "Indicates that a collaborative learning process includes the participation of a specific group" ;
               owl:versionInfo "Version 1. 0 - Initial definition of the property for associating groups with collaborative learning processes" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#isAchievedThroughProject
:isAchievedThroughProject rdf:type owl:ObjectProperty ,
                                   owl:AsymmetricProperty ,
                                   owl:IrreflexiveProperty ;
                          rdfs:domain :LearningOutcomes ;
                          rdfs:range :ProjectBasedLearning ;
                          <http://purl.org/dc/elements/1.1/description> "This property links learning outcomes with the project-based activities that facilitate their achievement" ;
                          rdfs:comment "Specifies the relationship where learning outcomes are attained through project-based learning activities" ;
                          owl:versionInfo "Version 1.1 - Added alignment with project-based learning methodologies" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#isAlignedWithCriteria
:isAlignedWithCriteria rdf:type owl:ObjectProperty ,
                                owl:AsymmetricProperty ,
                                owl:IrreflexiveProperty ;
                       rdfs:domain :LearningObject ;
                       rdfs:range :AssessmentCriteria ;
                       <http://purl.org/dc/elements/1.1/description> "This property establishes a link between a learning object and the criteria it adheres to for assessment purposes" ;
                       rdfs:comment "Indicates that a learning object aligns with specific assessment criteria, ensuring relevance and consistency" ;
                       owl:versionInfo "Version 1.1 - Refined to reflect specific alignment purposes in educational contexts" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#isAssessedByCriteria
:isAssessedByCriteria rdf:type owl:ObjectProperty ,
                               owl:AsymmetricProperty ,
                               owl:IrreflexiveProperty ;
                      rdfs:domain :LearningOutcomes ,
                                  :ProjectBasedLearning ;
                      rdfs:range :AssessmentCriteria ;
                      <http://purl.org/dc/elements/1.1/description> "Links learning outcomes or project-based learning activities to their respective assessment criteria, providing a basis for structured evaluation" ;
                      rdfs:comment "Denotes the relationship where learning outcomes or projects are evaluated based on specific assessment criteria, ensuring alignment and validity" ;
                      owl:versionInfo "Version 1.1 - Enhances the linkage between outcomes, projects, and their assessment standards" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#isBasedOnCriteria
:isBasedOnCriteria rdf:type owl:ObjectProperty ,
                            owl:AsymmetricProperty ,
                            owl:IrreflexiveProperty ;
                   rdfs:domain :IntelligentGroupPairing ;
                   rdfs:range :Group ;
                   <http://purl.org/dc/elements/1.1/description> "Represents the association between a grouping process or mechanism and the set of criteria used as its foundation" ;
                   rdfs:comment "Indicates that the formation or evaluation of a group is guided by specific predefined criteria" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#isEvaluatedThrough
:isEvaluatedThrough rdf:type owl:ObjectProperty ,
                             owl:AsymmetricProperty ,
                             owl:IrreflexiveProperty ;
                    rdfs:domain :AssessmentCriteria ;
                    rdfs:range :PeerFeedback ;
                    <http://purl.org/dc/elements/1.1/description> "This property indicates the evaluation process associated with specific assessment criteria, such as peer feedback or automated metrics" ;
                    rdfs:comment "Defines the mechanism or process through which assessment criteria are evaluated, often involving feedback or performance metrics" ;
                    owl:versionInfo "Version 1.0 - Initial definition: Establishes a clear link between assessment criteria and their evaluation mechanisms" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#isInformedByLearnerProfile
:isInformedByLearnerProfile rdf:type owl:ObjectProperty ,
                                     owl:FunctionalProperty ,
                                     owl:AsymmetricProperty ,
                                     owl:IrreflexiveProperty ;
                            rdfs:domain :Learner ;
                            <http://purl.org/dc/elements/1.1/description> "This property indicates the connection between learner-specific profiles and the decisions or actions taken in a personalized learning environment" ;
                            rdfs:comment "Represents the dependency of a decision or activity on the data captured within a learner's profile" ;
                            owl:versionInfo "Version 1.1 - Extended to include learner-specific adaptation" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#isLinkedToOutcome
:isLinkedToOutcome rdf:type owl:ObjectProperty ,
                            owl:AsymmetricProperty ,
                            owl:IrreflexiveProperty ;
                   rdfs:domain :AssessmentCriteria ;
                   rdfs:range :LearningOutcomes ;
                   <http://purl.org/dc/elements/1.1/description> "This property establishes the connection between specific assessment criteria and the learning outcomes they aim to measure or achieve" ;
                   rdfs:comment "Represents the association between assessment criteria and the corresponding learning outcomes they evaluate" ;
                   owl:versionInfo "Version 1.0 - Initial release: Designed to map assessment criteria to their respective learning outcomes for clear traceability" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#isMemberOf
:isMemberOf rdf:type owl:ObjectProperty ,
                     owl:IrreflexiveProperty ;
            rdfs:domain :Group ;
            rdfs:range :Learner ;
            <http://purl.org/dc/elements/1.1/description> "This property identifies the membership connection between a learner and a group, emphasizing the learner's participation or affiliation with the group" ;
            rdfs:comment "Denotes the relationship where a learner belongs to a specific group" ;
            owl:versionInfo "Version 1.0 - Initial release: Captures learner-group relationships for collaborative learning contexts" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#isProvidedByLearner
:isProvidedByLearner rdf:type owl:ObjectProperty ,
                              owl:FunctionalProperty ,
                              owl:AsymmetricProperty ,
                              owl:IrreflexiveProperty ;
                     rdfs:domain :PeerFeedback ;
                     rdfs:range :Learner ;
                     rdfs:comment "Captures the source of peer feedback by associating it with the learner who provided it, thereby facilitating traceability and accountability in the feedback system" ,
                                  "Indicates that peer feedback originates from a specific learner. This property highlights the contribution of individual learners to the feedback process in collaborative learning settings" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#isRelatedToGroup
:isRelatedToGroup rdf:type owl:ObjectProperty ,
                           owl:AsymmetricProperty ,
                           owl:IrreflexiveProperty ;
                  rdfs:domain :PeerFeedback ;
                  rdfs:range :Group ;
                  <http://purl.org/dc/elements/1.1/description> "Represents the association of peer feedback instances with groups, facilitating analysis of group-based evaluations and ensuring feedback relevance within collaborative settings" ;
                  rdfs:comment "Defines the relationship where peer feedback is associated with a specific group in the context of collaborative learning. This property establishes the link between the feedback provided and the corresponding group activities or members" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#isSharedAmongGroups
:isSharedAmongGroups rdf:type owl:ObjectProperty ,
                              owl:AsymmetricProperty ,
                              owl:IrreflexiveProperty ;
                     rdfs:domain :LearningResources ;
                     rdfs:range :Group ;
                     <http://purl.org/dc/elements/1.1/description> "Captures the relationship where a single learning resource is utilized collaboratively by different groups, fostering resource efficiency and group interaction" ;
                     rdfs:comment "Defines the sharing of specific learning resources among multiple groups within a collaborative learning environment" ;
                     owl:versionInfo "Version 1.0 - Establishes the shared usage of resources in group learning settings" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#isSupportedByResource
:isSupportedByResource rdf:type owl:ObjectProperty ,
                                owl:AsymmetricProperty ,
                                owl:IrreflexiveProperty ;
                       rdfs:domain :LearningOutcomes ;
                       rdfs:range :LearningResources ;
                       <http://purl.org/dc/elements/1.1/description> "Links learning outcomes to the resources that aid or provide support, ensuring access to relevant materials for achieving the outcomes" ;
                       rdfs:comment "Defines the relationship where learning outcomes are backed or facilitated by specific learning resources" ;
                       owl:versionInfo "Version 1.2 - Expands on the dependency between learning outcomes and their supporting resources" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#isUsedByLearner
:isUsedByLearner rdf:type owl:ObjectProperty ,
                          owl:AsymmetricProperty ,
                          owl:IrreflexiveProperty ;
                 rdfs:domain :LearningResources ;
                 rdfs:range :Learner ;
                 <http://purl.org/dc/elements/1.1/description> "Captures the interaction between learners and the learning resources they access or utilize to achieve educational goals" ;
                 rdfs:comment "Denotes the relationship where a specific learning resource is utilized by a learner during the learning process" ;
                 owl:versionInfo "Version 1.0 - Maps learner usage of resources in various learning activities" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#isUsedInProject
:isUsedInProject rdf:type owl:ObjectProperty ,
                          owl:AsymmetricProperty ,
                          owl:IrreflexiveProperty ;
                 rdfs:domain :LearningObject ;
                 rdfs:range :ProjectBasedLearning ;
                 <http://purl.org/dc/elements/1.1/description> "This property indicates the usage of a learning object in the context of project-based learning tasks, facilitating the alignment of resources with project objectives" ;
                 rdfs:comment "Specifies that a learning object is utilized within a specific project-based learning activity to achieve educational objectives" ;
                 owl:versionInfo "Version 1.1 - Updated to clarify its role in project-based learning scenarios" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#optimizesLearningOutcome
:optimizesLearningOutcome rdf:type owl:ObjectProperty ,
                                   owl:AsymmetricProperty ,
                                   owl:IrreflexiveProperty ;
                          rdfs:domain :IntelligentGroupPairing ;
                          rdfs:range :LearningOutcomes ;
                          <http://purl.org/dc/elements/1.1/description> "This property defines how intelligent grouping mechanisms are designed to improve learning results by aligning group characteristics with desired educational outcomes." ;
                          rdfs:comment "Represents the relationship where an intelligent group pairing process enhances the likelihood of achieving targeted learning outcomes" ;
                          owl:versionInfo "Version 1.2 - Extended for intelligent group pairing contexts" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#producesLearningOutcome
:producesLearningOutcome rdf:type owl:ObjectProperty ,
                                  owl:AsymmetricProperty ,
                                  owl:IrreflexiveProperty ;
                         rdfs:domain :CollaborativeLearning ;
                         rdfs:range :LearningOutcomes ;
                         <http://purl.org/dc/elements/1.1/description> "Defines the connection between collaborative learning processes and the generation of measurable learning outcomes" ;
                         rdfs:comment "Relates collaborative activities to their intended learning outcomes" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#providesFeedbackToGroup
:providesFeedbackToGroup rdf:type owl:ObjectProperty ,
                                  owl:AsymmetricProperty ,
                                  owl:IrreflexiveProperty ;
                         rdfs:domain :Group ;
                         rdfs:range :PeerFeedback ;
                         <http://purl.org/dc/elements/1.1/description> "Indicates the feedback mechanism directed toward a group to enhance group learning or performance" ;
                         rdfs:comment "Describes the action where feedback is provided specifically to a group, supporting collaborative evaluation and learning" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#providesPeerFeedback
:providesPeerFeedback rdf:type owl:ObjectProperty ,
                               owl:AsymmetricProperty ,
                               owl:IrreflexiveProperty ;
                      rdfs:domain :Learner ;
                      rdfs:range :PeerFeedback ;
                      <http://purl.org/dc/elements/1.1/description> "This property identifies the interaction where a learner actively gives constructive feedback to their peers as part of collaborative or reflective learning activities" ;
                      rdfs:comment "Represents the relationship in which a learner provides feedback on the work or performance of their peers" ;
                      owl:versionInfo "Version 1.1 - Introduced to enhance peer interaction representation in learning activities" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#supportsLearningOutcome
:supportsLearningOutcome rdf:type owl:ObjectProperty ,
                                  owl:AsymmetricProperty ,
                                  owl:IrreflexiveProperty ;
                         rdfs:domain :LearningObject ;
                         rdfs:range :LearningOutcomes ;
                         <http://purl.org/dc/elements/1.1/description> "This property links learning objects with the learning outcomes they aim to support or enhance" ;
                         rdfs:comment "Defines the relationship where a learning object provides support in achieving specific learning outcomes" ;
                         owl:versionInfo "Version 1.2 - Improved alignment with instructional design frameworks" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#usesLearningObject
:usesLearningObject rdf:type owl:ObjectProperty ,
                             owl:AsymmetricProperty ,
                             owl:IrreflexiveProperty ;
                    rdfs:domain :ProjectBasedLearning ;
                    rdfs:range :LearningObject ;
                    <http://purl.org/dc/elements/1.1/description> "This property indicates that a specific project-based learning task incorporates certain learning objects, such as interactive multimedia, documents, or other resources, to support the educational process" ;
                    rdfs:comment "Defines the relationship between a project-based learning activity and the learning objects utilized within the project to achieve educational objectives" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#usesLearningResources
:usesLearningResources rdf:type owl:ObjectProperty ,
                                owl:AsymmetricProperty ,
                                owl:IrreflexiveProperty ;
                       rdfs:domain :CollaborativeLearning ,
                                   :Learner ;
                       rdfs:range :LearningResources ;
                       <http://purl.org/dc/elements/1.1/description> "Defines the connection between collaborative learning processes and the generation of measurable learning outcomes." ;
                       rdfs:comment "Specifies the relationship between a learner or a collaborative learning activity and the educational resources employed to achieve learning objectives. This property emphasizes the utilization of resources in educational activities" .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#AccessRights
:AccessRights rdf:type owl:Class ;
              rdfs:subClassOf :Security .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#Account
:Account rdf:type owl:Class ;
         rdfs:subClassOf :Security .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#AssessmentCriteria
:AssessmentCriteria rdf:type owl:Class ;
                    <http://purl.org/dc/elements/1.1/description> "This class specifies the parameters and benchmarks for evaluating the effectiveness of collaborative learning activities, including criteria such as creativity, critical thinking, and problem-solving skills" ;
                    rdfs:comment "Defines the standards or benchmarks used to evaluate learner or group performance" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#BelongsTo
:BelongsTo rdf:type owl:Class ;
           rdfs:subClassOf :Relations .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#CollaborativeLearning
:CollaborativeLearning rdf:type owl:Class ;
                       <http://purl.org/dc/elements/1.1/description> "This class models learning scenarios where participants engage in group-based activities to achieve shared learning goals. Examples include group discussions, team-based projects, and cooperative problem-solving tasks" ;
                       rdfs:comment "Represents learning activities that involve group collaboration" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#CommunicationSkills
:CommunicationSkills rdf:type owl:Class ;
                     rdfs:subClassOf :LearningOutcomes ;
                     <http://purl.org/dc/elements/1.1/description> "Communication skills encompass the ability of learners to effectively articulate ideas, share knowledge, and engage in meaningful interactions in various contexts, including group discussions, collaborative projects, and professional settings. These skills are essential for fostering teamwork, resolving conflicts, and conveying critical information in both written and verbal forms" ;
                     rdfs:comment "This class captures the learning outcomes related to developing and enhancing a learner's capacity for interpersonal communication, presentation delivery, and active listening. It includes both synchronous and asynchronous communication scenarios and supports assessments that evaluate clarity, coherence, and the ability to engage effectively with peers" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#Competencies
:Competencies rdf:type owl:Class ;
              rdfs:subClassOf :Performance .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#CooperativeProblemSolving
:CooperativeProblemSolving rdf:type owl:Class ;
                           rdfs:subClassOf :CollaborativeLearning ;
                           <http://purl.org/dc/elements/1.1/description> "This class focuses on fostering teamwork and collaboration to address complex problems, leveraging the collective skills and perspectives of group members to achieve shared learning objectives" ;
                           rdfs:comment "Represents activities where learners collaborate to identify, analyze, and solve problems as a team, emphasizing shared responsibilities and group dynamics" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#Creativity
:Creativity rdf:type owl:Class ;
            rdfs:subClassOf :AssessmentCriteria ;
            <http://purl.org/dc/elements/1.1/description> "This class defines a critical assessment criterion focusing on learners' capacity to think divergently and produce original solutions during collaborative projects" ;
            rdfs:comment "Represents the ability to generate innovative ideas and approaches in collaborative learning activities" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#CriticalThinking
:CriticalThinking rdf:type owl:Class ;
                  rdfs:subClassOf :AssessmentCriteria ;
                  <http://purl.org/dc/elements/1.1/description> "This class emphasizes an essential skill for assessing a learner's ability to evaluate data, arguments, and processes critically to enhance decision-making in collaborative learning environments" ;
                  rdfs:comment "Represents the ability to analyze information objectively, identify logical relationships, and make reasoned judgments within collaborative learning tasks" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#CurrentPBCL
:CurrentPBCL rdf:type owl:Class ;
             rdfs:subClassOf :PortfolioInfo .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#DiversityBasedPairing
:DiversityBasedPairing rdf:type owl:Class ;
                       rdfs:subClassOf :IntelligentGroupPairing ;
                       <http://purl.org/dc/elements/1.1/description> "This class focuses on the creation of diverse groups to maximize learning opportunities by incorporating members with varied skills, backgrounds, or experiences. The diversity-based pairing approach ensures balanced and inclusive group dynamics" ;
                       rdfs:comment "Defines a pairing strategy that promotes diversity among group members by considering various demographic, academic, or personal attributes" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#Group
:Group rdf:type owl:Class ;
       <http://purl.org/dc/elements/1.1/description> "This class captures the structure of learning groups, including attributes such as group size, composition, and performance. Groups can be dynamically formed based on specific criteria" ;
       rdfs:comment "Represents a collection of learners working together in collaborative learning activities" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#GroupDiscussions
:GroupDiscussions rdf:type owl:Class ;
                  rdfs:subClassOf :CollaborativeLearning ;
                  <http://purl.org/dc/elements/1.1/description> "This class focuses on fostering communication, idea exchange, and mutual understanding among learners through organized discussion sessions as part of collaborative learning" ;
                  rdfs:comment "Represents collaborative activities where group members engage in structured conversations to share ideas, debate topics, and collectively explore solutions" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#GroupInfo
:GroupInfo rdf:type owl:Class ;
           rdfs:subClassOf :Group ;
           <http://purl.org/dc/elements/1.1/description> "This class includes key attributes that define a group's structure and characteristics, such as name, size, and other relevant data for collaborative tasks. It serves as a foundational element for understanding and organizing group dynamics" ;
           rdfs:comment "Encapsulates metadata and descriptive details about a specific group participating in collaborative learning" .


###  http://www.semanticweb.org/asmah/ontologies/2023/6/untitled-ontology-25#GroupName
:GroupName rdf:type owl:Class ;
           rdfs:subClassOf :GroupInfo .


